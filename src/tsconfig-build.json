{
  "compilerOptions": {
    "downlevelIteration": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "declaration": true,
    "stripInternal": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "../dist/",
    "paths": {},
    "rootDir": ".",
    "sourceMap": true,
    "inlineSources": true,
    "target": "es2015",
    "lib": [
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": false
  },
  "files": [
    "public_api.ts"
  ],
  "angularCompilerOptions": {
    "flatModuleOutFile": "index.js",
    "flatModuleId": "ngx-jsonapi",
    "enableIvy": true,
    "allowEmptyCodegenFiles": true
  }
}